<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/gradiente_color"
    android:orientation="vertical"
    app:layout_constraintCircleRadius="20dp"
    tools:context=".CadastroParte2">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="104dp"
            android:layout_height="105dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.052"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.043"
            app:srcCompat="@drawable/frimmel_transparente" />

        <TextView
            android:id="@+id/textFrimmelFC2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/itim"
            android:text="Frimmel"
            android:textAllCaps="true"
            android:textColor="#5A6B59"
            android:textSize="30sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.09" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintLayout3"
            android:layout_width="409dp"
            android:layout_height="587dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="1.0">

            <Button
                android:id="@+id/VoltarCadastro2"
                android:layout_width="130dp"
                android:layout_height="60dp"
                android:backgroundTint="#B8D4B6"
                android:fontFamily="@font/itim"
                android:text="Voltar"
                android:textColor="#D43535"
                android:textSize="18sp"
                app:cornerRadius="20dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.813"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.882" />

            <EditText
                android:id="@+id/CadastroEndereço"
                android:layout_width="306dp"
                android:layout_height="62dp"
                android:background="@drawable/background_corner"
                android:drawableLeft="@drawable/baseline_pin_drop_24"
                android:drawablePadding="6dp"
                android:ems="10"
                android:fontFamily="@font/itim"
                android:hint="Endereço"
                android:inputType="text"
                android:textColor="#1A8414"
                android:textColorHint="#1A8414"
                android:textIsSelectable="false"
                android:textSize="18sp"
                app:circularflow_radiusInDP="20sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.495"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.407" />

            <EditText
                android:id="@+id/CadastroPlano"
                android:layout_width="306dp"
                android:layout_height="62dp"
                android:background="@drawable/background_corner"
                android:drawableLeft="@drawable/baseline_signal_wifi_4_bar_24"
                android:drawablePadding="6dp"
                android:ems="10"
                android:fontFamily="@font/itim"
                android:hint="Plano"
                android:inputType="text"
                android:textColor="#1A8414"
                android:textColorHint="#1A8414"
                android:textIsSelectable="false"
                android:textSize="18sp"
                app:circularflow_radiusInDP="20sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.495"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.561" />

            <EditText
                android:id="@+id/CadastroCEP"
                android:layout_width="306dp"
                android:layout_height="62dp"
                android:background="@drawable/background_corner"
                android:drawableLeft="@drawable/baseline_person_24"
                android:drawablePadding="6dp"
                android:ems="10"
                android:fontFamily="@font/itim"
                android:hint="CEP"
                android:inputType="text"
                android:textColor="#1A8414"
                android:textColorHint="#1A8414"
                android:textIsSelectable="false"
                android:textSize="18sp"
                app:circularflow_radiusInDP="20sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.495"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.26" />

            <TextView
                android:id="@+id/textNovaSenha"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/itim"
                android:text="Cadastro do Cliente"
                android:textColor="#5A6B59"
                android:textSize="30sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.496"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.09" />

            <EditText
                android:id="@+id/CadastroSenha"
                android:layout_width="306dp"
                android:layout_height="62dp"
                android:background="@drawable/background_corner"
                android:drawableLeft="@drawable/baseline_lock_24"
                android:drawablePadding="6dp"
                android:ems="10"
                android:fontFamily="@font/itim"
                android:hint="Senha"
                android:inputType="textPassword"
                android:textColor="#1A8414"
                android:textColorHint="#1A8414"
                android:textSize="18sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.504"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.714" />

            <Button
                android:id="@+id/ContinuarCadastro2"
                android:layout_width="130dp"
                android:layout_height="60dp"
                android:backgroundTint="#B8D4B6"
                android:fontFamily="@font/itim"
                android:text="Confirmar"
                android:textColor="#1A8414"
                android:textSize="18sp"
                app:cornerRadius="20dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.182"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.882" />


        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>
</LinearLayout>