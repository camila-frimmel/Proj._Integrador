<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/gradiente_color"
    tools:context=".ManutençãoCliente">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="409dp"
        android:layout_height="729dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="1dp"
        android:layout_marginBottom="1dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="104dp"
            android:layout_height="105dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="33dp"
            android:layout_marginBottom="591dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/frimmel_transparente" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintLayout"
            android:layout_width="415dp"
            android:layout_height="587dp"
            android:layout_marginBottom="1dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent">

            <EditText
                android:id="@+id/editHorario"
                android:layout_width="306dp"
                android:layout_height="62dp"
                android:layout_marginStart="54dp"
                android:layout_marginTop="40dp"
                android:layout_marginEnd="54dp"
                android:background="@drawable/background_corner"
                android:ems="10"
                android:fontFamily="@font/itim"
                android:hint="Horário"
                android:inputType="time"
                android:textAlignment="center"
                android:textColor="#1A8414"
                android:textColorHint="#AD1A8414"
                android:textSize="35dp"
                android:textStyle="bold"
                app:layout_constraintBottom_toTopOf="@+id/editDia"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.0" />

            <EditText
                android:id="@+id/editDia"
                android:layout_width="306dp"
                android:layout_height="62dp"
                android:layout_marginStart="54dp"
                android:layout_marginEnd="54dp"
                android:layout_marginBottom="20dp"
                android:background="@drawable/background_corner"
                android:ems="10"
                android:fontFamily="@font/itim"
                android:hint="Dia da Semana"
                android:inputType="time"
                android:textAlignment="center"
                android:textColor="#1A8414"
                android:textColorHint="#AD1A8414"
                android:textSize="35dp"
                android:textStyle="bold"
                app:layout_constraintBottom_toTopOf="@+id/ViewEndereço"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent" />

            <TextView
                android:id="@+id/ViewEndereço"
                android:layout_width="306dp"
                android:layout_height="65dp"
                android:layout_marginStart="54dp"
                android:layout_marginEnd="54dp"
                android:layout_marginBottom="56dp"
                android:background="@drawable/background_corner"
                android:fontFamily="@font/itim"
                android:hint="Endereço"
                android:paddingVertical="10dp"
                android:textAlignment="center"
                android:textColor="#1A8414"
                android:textColorHint="#1A8414"
                android:textSize="35dp"
                android:textStyle="bold"
                app:layout_constraintBottom_toTopOf="@+id/editTextTextMultiLine"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent" />

            <TextView
                android:id="@+id/textView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="29dp"
                android:layout_marginTop="290dp"
                android:layout_marginEnd="68dp"
                android:layout_marginBottom="257dp"
                android:text="Motivo da Manutenção:"
                android:textColor="#5A6B59"
                android:textSize="30sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <EditText
                android:id="@+id/editTextTextMultiLine"
                android:layout_width="363dp"
                android:layout_height="233dp"
                android:layout_marginStart="29dp"
                android:layout_marginEnd="23dp"
                android:layout_marginBottom="24dp"
                android:background="@drawable/background_corner"
                android:ems="10"
                android:fontFamily="@font/itim"
                android:gravity="start|top"
                android:inputType="textMultiLine"
                android:textAlignment="viewStart"
                android:textColor="#1A8414"
                android:textSize="30dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <TextView
            android:id="@+id/textFrimmelMC"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="68dp"
            android:layout_marginEnd="141dp"
            android:fontFamily="@font/itim"
            android:text="Frimmel"
            android:textAllCaps="true"
            android:textColor="#5A6B59"
            android:textSize="30sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/constraintLayout"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toEndOf="@+id/imageView"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>